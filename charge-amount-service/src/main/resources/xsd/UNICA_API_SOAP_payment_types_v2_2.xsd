<?xml version="1.0" encoding="UTF-8"?><xsd:schema attributeFormDefault="unqualified"	elementFormDefault="qualified"	targetNamespace="http://www.telefonica.com/schemas/UNICA/SOAP/payment/v2/types"	xmlns:tns="http://www.telefonica.com/schemas/UNICA/SOAP/payment/v2/types"	xmlns:xsd="http://www.w3.org/2001/XMLSchema">	<xsd:annotation>		<xsd:documentation>Business data</xsd:documentation>	</xsd:annotation>	<xsd:complexType name="ExtensionType">		<xsd:sequence>			<xsd:element name="vodProductAdditionalInfo"				type="tns:VodProductAdditionalInfoType" />		</xsd:sequence>		<xsd:anyAttribute />	</xsd:complexType>	<!-- VoD product additional Info, to be used in other APIs when needed -->	<xsd:complexType name="VodProductAdditionalInfoType">		<xsd:group ref="tns:VodProductAdditionalInfoGroup" />	</xsd:complexType>	<!-- The following element may be used in other APIs within an ExtensionType 		element -->	<xsd:element name="vodProductAdditionalInfo"		type="tns:VodProductAdditionalInfoType" />	<!-- Ppv product additional Info, to be used in other APIs when needed -->	<!-- The following element may be used in other APIs within an ExtensionType 		element -->	<!-- Voucher product additional Info, to be used in other APIs when needed -->	<!-- The following element may be used in other APIs within an ExtensionType 		element -->	<!-- Specific video products additional Info that can be used along with 		other APIs when no product catalog is available -->	<xsd:group name="VodProductAdditionalInfoGroup">		<xsd:sequence>			<xsd:element minOccurs="0" ref="tns:genre" />			<xsd:element minOccurs="0" ref="tns:producer" />		</xsd:sequence>	</xsd:group>	<!-- Detailed elements -->	<xsd:element name="genre">		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="64" />			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:element name="producer">		<xsd:simpleType>			<xsd:restriction base="xsd:string">				<xsd:maxLength value="64" />			</xsd:restriction>		</xsd:simpleType>	</xsd:element>	<xsd:complexType name="UserIdType">		<xsd:choice>			<xsd:element name="alias" type="tns:AliasType" />		</xsd:choice>	</xsd:complexType>	<xsd:simpleType name="AliasType">		<xsd:restriction base="xsd:string" />	</xsd:simpleType>	<xsd:complexType name="ClientExceptionType">		<xsd:annotation>			<xsd:documentation>Returned by server in case of Client related				faults			</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="exceptionCategory">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="SVC" />						<xsd:enumeration value="POL" />						<xsd:enumeration value="SEC" />					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="exceptionId">				<xsd:simpleType>					<xsd:restriction base="xsd:int">						<xsd:minInclusive value="0000" />						<xsd:maxInclusive value="9999" />					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="text" type="xsd:string" />			<xsd:element maxOccurs="unbounded" minOccurs="0"				name="variables" type="xsd:string" />		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ServerExceptionType">		<xsd:annotation>			<xsd:documentation>Object returned by server in case of Server				related faults			</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="exceptionCategory">				<xsd:simpleType>					<xsd:restriction base="xsd:string">						<xsd:enumeration value="SVR" />					</xsd:restriction>				</xsd:simpleType>			</xsd:element>			<xsd:element name="exceptionId" type="xsd:int" />			<xsd:element name="text" type="xsd:string" />			<xsd:element maxOccurs="unbounded" minOccurs="0"				name="variables" type="xsd:string" />		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ChargingInformationType">		<xsd:sequence>			<xsd:element maxOccurs="unbounded" minOccurs="1"				name="description" type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0" name="amount"				type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="0" name="code"				type="xsd:string" />		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="ChargingMetaDataType">		<xsd:sequence>			<xsd:element maxOccurs="1" minOccurs="0"				name="purchaseCategoryCode" type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0" name="channel"				type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0" name="serviceId"				type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0" name="productId"				type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0"				name="clientDate" type="xsd:dateTime" />			<xsd:element maxOccurs="1" minOccurs="0"				name="additionalProductInfo" type="tns:ExtensionType" />		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="AmountRateType">		<xsd:sequence>			<xsd:element maxOccurs="1" minOccurs="1" name="amount"				type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="1" name="interval"				type="xsd:duration" />		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="TransactionStatusDataType">		<xsd:annotation>			<xsd:documentation>Transaction status data information (managed by				server)			</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element maxOccurs="1" minOccurs="1"				name="transactionId" type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="1"				name="transactionOperationStatus"				type="tns:TransactionOperationStatusType" />			<xsd:element maxOccurs="1" minOccurs="0"				name="transactionOperationStatusDescription" type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0" name="paymentId"				type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0"				name="clientException" type="tns:ClientExceptionType" />			<xsd:element maxOccurs="1" minOccurs="0"				name="serverException" type="tns:ServerExceptionType" />			<xsd:element maxOccurs="1" minOccurs="0"				name="serverReferenceCode" type="xsd:string" />			<xsd:element maxOccurs="1" minOccurs="0"				name="totalAmountCharged" type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="0"				name="totalAmountRefunded" type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="0"				name="totalAmountCredited" type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="0"				name="amountReserved" type="xsd:decimal" />			<xsd:element maxOccurs="1" minOccurs="0"				name="expirationDate" type="xsd:dateTime" />		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="TransactionOperationStatusType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Processing" />			<xsd:enumeration value="AuthorizationInProgress" />			<xsd:enumeration value="Authorized" />			<xsd:enumeration value="Charged" />			<xsd:enumeration value="Refunded" />			<xsd:enumeration value="Credited" />			<xsd:enumeration value="Reserved" />			<xsd:enumeration value="Released" />			<xsd:enumeration value="Denied" />			<xsd:enumeration value="Refused" />			<xsd:enumeration value="Unauthorized" />			<xsd:enumeration value="Terminated" />		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType		name="MultipleTransactionOperationStatusType">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="Processing" />			<xsd:enumeration value="Processed" />			<xsd:enumeration value="Failed" />		</xsd:restriction>	</xsd:simpleType>	<xsd:annotation>		<xsd:documentation>Operations</xsd:documentation>	</xsd:annotation>	<!--Amount Payment -->	<!--Amount Charging -->	<xsd:element name="chargeAmountRequest">		<xsd:complexType>			<xsd:annotation>				<xsd:documentation>Charge Amount request</xsd:documentation>			</xsd:annotation>			<xsd:sequence>				<xsd:element maxOccurs="1" minOccurs="1"					name="endUserIdentifier" type="tns:UserIdType" />				<xsd:element maxOccurs="1" minOccurs="1"					name="chargingInformation" type="tns:ChargingInformationType" />				<xsd:element maxOccurs="1" minOccurs="0"					name="chargingMetaData" type="tns:ChargingMetaDataType" />				<xsd:element maxOccurs="1" minOccurs="0"					name="referenceCode" type="xsd:string" />			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="chargeAmountResponse">		<xsd:complexType>			<xsd:annotation>				<xsd:documentation>Charge Amount response</xsd:documentation>			</xsd:annotation>			<xsd:sequence>				<xsd:element maxOccurs="1" minOccurs="1"					name="transactionId" type="xsd:string" />				<xsd:element maxOccurs="1" minOccurs="1"					name="transactionOperationStatus"					type="tns:TransactionOperationStatusType" />				<xsd:element maxOccurs="1" minOccurs="1"					name="totalAmountCharged" type="xsd:decimal" />			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<!--Amount Reservation -->	<!--Transaction Management -->	<!--Multiple Amount Charging -->	<!--Multiple Transaction Management --></xsd:schema>